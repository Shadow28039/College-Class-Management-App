<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finalproject2">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalProject2">
        <receiver
            android:name=".User_Interface.MyReceiver"
            android:enabled="true"
            android:exported="true"></receiver>

        <activity android:name=".User_Interface.Create_Course_Activity" />
        <activity android:name=".User_Interface.Create_Term_Activity" />
        <activity android:name=".User_Interface.Create_Assessment_Activity" />
        <activity android:name=".User_Interface.View_Term_Activity" />
        <activity android:name=".User_Interface.View_Course_Activity" />
        <activity android:name=".User_Interface.View_Mentor_Activity" />
        <activity android:name=".User_Interface.View_Assessment_Activity" />
        <activity android:name=".User_Interface.Term_Activity" />
        <activity android:name=".User_Interface.Edit_Term_Activity" />
        <activity android:name=".User_Interface.Edit_Course_Activity" />
        <activity android:name=".User_Interface.Edit_Mentor_Activity" />
        <activity android:name=".User_Interface.Edit_Note_Activity" />
        <activity android:name=".User_Interface.Edit_Assessment_Activity" />
        <activity android:name=".User_Interface.EmailSender" />
        <activity android:name=".User_Interface.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>